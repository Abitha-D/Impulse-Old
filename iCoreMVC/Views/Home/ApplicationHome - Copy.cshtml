    <script>
        var SubModulePageAccessFlag = false;

        function BindDesignation() {
            var Condition = ' WHERE role_id=' + access_level + '';
            var strJsonDatas = eval({ strSessionID: UserSession, strCondition: Condition, strFieldNames: "" });
            var Result = oAPICall.SelectData(strJsonDatas, 'SelectRoleAccessType');
            if (Result != null && Result.length > 0) {
                document.getElementById("titleuname_desig").innerHTML = Result[0]["role_name"];
                document.getElementById("titleuname_desig1").innerHTML = Result[0]["role_name"];
            }
        }


        function BindNavigationBar() {
            var strRoleID = "1";
            var moduleID = "";
            var Condition = ' WHERE user_id=' + CurrentUser + '';
            var strJsonDatas = eval({ strSessionID: UserSession, strCondition: Condition, strFieldNames: FieldNames });
            var Result = oAPICall.SelectData(strJsonDatas, 'SelectBtsUserMaster');
            if (Result != null && Result.length > 0) {
                strRoleID = Result[0]["role_id"];
            }
            var FieldNames = 'DISTINCT is_active,module_image_class,display_value,module_id';
            var Condition = 'WHERE role_id=' + strRoleID + ' ORDER BY module_id';
            var strJsonDatas = eval({ strSessionID: UserSession, strCondition: Condition, strFieldNames: FieldNames });
            var Modules = oAPICall.SelectData(strJsonDatas, 'SelectMainModules');
            $('#navigation-bar').html("");

            if (Modules != '' && Modules != null) {
                for (var i = 0; i < Modules.length; i++) {
                    var active = "";
                    var collaps = "collapse";
                    if (Modules[i]["is_active"] == Modules[i]["module_id"]) {
                        active = "active";
                        collaps = "";
                        moduleID = Modules[i]["module_id"];
                    }
                    var imgclass = Modules[i]["module_image_class"];

                    var toAppendModule = [];
                    toAppendModule.push(
                        "<li class=" + active + ">",
                        " <a href='#'><i class='" + imgclass + "'></i> <span class='nav-label'>" + Modules[i]["display_value"] + "</span> <span class='fa arrow'></span></a>",
                       "<ul style='overflow-y: scroll;max-height:300px;' class='nav nav-second-level " + collaps + "'  module_id='" + Modules[i]["module_id"] + "_module' >",
                        "</ul>",
                        "</li>"
                        );
                    $('#navigation-bar').append(toAppendModule.join(""));

                    //Submodule bind

                    //var FieldNames = 'DISTINCT sub_module_id, sub_module_name, sub_module_url ';
                    var FieldNames = 'DISTINCT sub_module_id, sub_display_value, sub_module_src,list_priority,module_id';
                    //var Condition = 'WHERE module_id=' + Modules[i]["module_id"] + ' AND menu_id=' + menu_id + ' AND language_code=' + CompanyLanguage + ' AND ( role_id in (' + strRoleID + ') OR role_id=' + CurrentUser + ')' + 'AND permission_id = 4 AND is_enabled = 1' + ' ORDER BY sub_module_id';
                    var Condition = 'WHERE module_id=' + Modules[i]["module_id"] + ' AND role_id=' + strRoleID + ' ORDER BY list_priority';
                    var strJsonDatas = eval({ strSessionID: UserSession, strCondition: Condition, strFieldNames: FieldNames });
                    var SubModules = oAPICall.SelectData(strJsonDatas, 'SelectMainModules');
                    for (var j = 0; j < SubModules.length; j++) {
                        var toAppendSubModule = [];
                        toAppendSubModule.push(

                            "<li><a class='sub_module' href='#' id='" + SubModules[j]["sub_module_id"] + "_sub_module_id" + "'title='" + Modules[i]["display_value"] + "' onclick='SubModuleMenuClick(this)' sub_module_url='" + SubModules[j]["sub_module_src"] + "'>" + SubModules[j]["sub_display_value"] + "<span class='pull-right id='" + SubModules[j]["sub_module_id"] + "_subtotal" + "'>&nbsp;</span></a></li>"

                                //"<li><a class='menu-item' id='" + SubModules[j]["sub_module_id"] + "_sub_module_id" + "' onclick='SubModuleMenuClick(this)' href='#' sub_module_url='" + SubModules[j]["sub_module_url"] + "'>" + SubModules[j]["sub_module_name"] + "</a></li>"
                            );
                        $('ul.nav').find("[module_id='" + Modules[i]["module_id"] + "_module']").append(toAppendSubModule.join());
                        if (j == 0 && SubModules[j]["module_id"] == moduleID) {
                            $("#" + SubModules[j]["sub_module_id"] + "_sub_module_id").click();
                        }
                    }
                }


            }
            $.getScript('../Scripts/js/plugins/metisMenu/jquery.metisMenu.js');

            $('#navigation-bar').metisMenu();

        }

        function SubModuleMenuClick(menu_item) {
            //SubModulePageAccess(menu_item.id.split("_sub_module_id")[0]);
            //if (!SubModulePageAccessFlag) {
            //    alert("You do not have access to this page !\nContact the Administrator.");
