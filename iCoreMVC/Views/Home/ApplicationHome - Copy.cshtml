        function BindNotificationContainer() {
            $(".notif-count").text('');
            var sCondition = " WHERE delete_status=0 and order_status = 1 ";
            if (access_level == 1) {
                var sRtnCondition = " WHERE delete_status=0 and order_status = 1 order by 1 desc";
            }
            else if (access_level == 2) {
                var sRtnCondition = sCondition + ' and order_posted_by = ' + EmpId + 'order by 1 desc';
            }
            else if (access_level == 3) {
                var sRtnCondition = sCondition + ' and photographer_id = ' + EmpId + 'order by 1 desc';
            }
            else if (access_level == 4) {
                var sRtnCondition = sCondition + ' and dataentry_id = ' + EmpId + 'order by 1 desc';
            }
            var strJsonDatas = eval({ strCondition: sRtnCondition, strFieldNames: null, strSessionID: UserSession });
            var Result = oAPICall.SelectData(strJsonDatas, "SelectBtsOrderList");
            var NotifCount = 0;
            $("#notification-container").html('');
            if (Result != "" && Result != null && Result.length > 0) {

                //var sLastTime = new Date(Result[0].order_date)
                var sLastTime = Result[0].order_date;
                var splitarray = new Array();
                splitarray = sLastTime.split("T");

                var sCtime = new Date(); // for now
                sCtime.getHours(); // => 9
                sCtime.getMinutes(); // =>  30
                sCtime.getSeconds();

                var lHrs = splitarray[1].split(":")

                var sLst = (sCtime.getMinutes() - lHrs[1]);
                var sDatetb = '';
                var cudate = new Date();
                //cudate = FormatCurrentDate('mm/dd/yyyy', cudate);
                //sLastTime = FormatCurrentDate('mm/dd/yyyy', sLastTime);

                cudate = dateFormat(cudate, "isoDateTime");
                sLastTime = dateFormat(sLastTime, "isoDateTime");

                if (parseInt(sCtime.getHours()) - parseInt(lHrs) > 1) {
                    $(".lastordertime").text('One hours ago');
                }
                else {
                    if (cudate > sLastTime) {             //parseInt(sCurDt) > parseInt(sltdt)
                        $(".lastordertime").text('One hours ago');
                    }
                    else {
                        if (sLst < 0)
                            sLst = (sLst * (-1));
                        $(".lastordertime").text(sLst + ' minutes ago');
                    }
                }
                NotifCount = parseInt(Result.length);
                $(".notif-count").text(NotifCount);
                var toAppend = [];
                toAppend.push(
                "<header class='panel-heading'>",
                "<strong>From Orders</strong>",
                "</header>"
                );

                for (var i = 0; i < Result.length; i++) {
                    var sOrderNo = Result[i].order_no;
                    var sOrderCode = Result[i].order_code;
                    toAppend.push(
                      "<a href='#'  onclick='PopulateOrder(" + sOrderNo + ")' class='media list-group-item' data-title='New Order Creation'>",
                    "" + sOrderCode + ' (' + Result[i].subject_address + ")",
                    "</a>"
                    );
                }
                $("#notification-container").append(toAppend.join(""));
            }
        }


