            cell5.appendChild(element5);

            var cell6 = row.insertCell(5);
            var element6 = document.createElement("input");
            element6.type = "text";
            element6.id = "txtexpcNo";
            element6.style.width = "80px";
            cell6.appendChild(element6);

            var cell7 = row.insertCell(6);
