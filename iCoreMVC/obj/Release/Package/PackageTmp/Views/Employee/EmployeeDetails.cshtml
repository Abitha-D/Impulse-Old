                    EduDivShow("sslcDoc",Result[0]["sslc_doc"]);
                }

                $("#lbltwelthper").text(Result[0]["twelth_mark"]);

                $("#ddlVTwelthSyllabus").val(Result[0]["twelth_syllabus"]).change();

                $("#lbltwelthyop").text(Result[0]["twelth_pass_year"]);

                if(Result[0]["twelth_doc"] != '')
