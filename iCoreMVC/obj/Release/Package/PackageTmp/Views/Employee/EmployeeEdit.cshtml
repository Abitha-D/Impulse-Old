    <script>

        function naChange(id)
        {
            if(id == "pfNA")
            {

                if(document.getElementById("pfNA").checked == true)
                {
                    var GrossPay = $('#txtSalGrossPay').val();

                    var incomeTax = $('#txtSalIncometax').val();

                    var others = $('#txtSalOthers').val();

                    var ESIemp = $('#txtSalESIemp').val();

                    var gratuity = $('#txtSalGratuity').val();

                    var Annual = $('#txtSalAnnual').val();

                    var esi = $('#txtSalESI').val();

                    var pf = 0;

                    var totalDeduction = parseInt(pf + esi);

                    var NetPay = parseFloat(GrossPay) - (parseFloat(pf) + parseFloat(esi) + parseFloat(incomeTax) + parseFloat(others));

                    var TotalCost = parseFloat(GrossPay) + parseFloat(pf) + parseFloat(ESIemp) + parseFloat(gratuity) + parseFloat(Annual);

                    $('#txtSalPF').val(pf);

                    $('#txtSalPFemp').val(pf);

                    $('#txtSalDeduction').val(totalDeduction);

                    $('#txtSalNetPay').val(NetPay);

                    $('#txtSalTotalCost').val(TotalCost);
                }
                else if(document.getElementById("pfNA").checked == false)
                {
                    var GrossPay = $('#txtSalGrossPay').val();

                    var incomeTax = $('#txtSalIncometax').val();

                    var others = $('#txtSalOthers').val();

                    var ESIemp = $('#txtSalESIemp').val();

                    var gratuity = $('#txtSalGratuity').val();

                    var Annual = $('#txtSalAnnual').val();

                    var esi = $('#txtSalESI').val();

                    var hra = $('#txtSalHRA').val();

                    if ((GrossPay - hra) >= 15000) {
